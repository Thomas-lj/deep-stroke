{
  "exp": {
    "name": "test"
  },
  "model":{
    "name": "model.LMModel",
	"bash_name": "heart-50epoch",
    "CNN_window_size": 37500,
    "dense_units":{
      "DenseLayer1": 128,
      "DenseLayer2": 128
    },
    "alpha": 1,
    "weights": "None",
    "dimensionality":{
      "Layer1": 50,
      "Layer2": 40
    },
    "initial_lr": 1e-4,
    "input_shape": [3, 1, 37500],
    "kernel_size":{
      "Layer1": 10,
      "Layer2": 20
    },
    "learning_rate": 0.001,
    "num_gpus": 1,
    "num_LSTM_units": {
      "Layer1": 15,
      "Layer2": 1
    },
    "num_time_steps": 1,
    "optimizer": "adam",
    "loss_func": "LeakyReLU",
    "padding":{
      "Layer1": "same",
      "Layer2": "same"
    },
    "scale_l2_regularization": 0.29586,
    "stride":{
      "Layer1": 1,
      "Layer2": 2
    },
    "pool_size": 1
  },
  "trainer":{
    "name": "trainer.TemporalTrainer",
    "alpha": 1,
    "n_clusters": 2,
    "iter_max": 50,
    "num_epochs": 50,
    "batch_size": 64,
    "validation_split":0.25,
    "verbose_training": 2,
    "num_workers": 20,
    "use_multiprocessing": true,
    "max_queue_size": 512,
    "class_weight": {"0": 1, "1": 1}
  },
  "callbacks":{
    "checkpoint_monitor": "val_loss",
    "checkpoint_mode": "min",
    "checkpoint_save_best_only": true,
    "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "earlystopping_monitor": "val_loss",
    "encoder_pred_group": "train_data",
    "earlystopping_patience": 10,
    "earlystopping_verbose": true,
    "learningrate_decay_rate": 0.96,
    "learningrate_decay_epochs": 15,
    "tensorboard_write_graph": true
  },
  "data_loader":{
    "name": "DataGenerator.DataGenerator",
    "batch_size":{
    "train": 64,
    "eval": 64,
    "test": 64
    },
    "data_dir": "./data/h5_stroke",
    "channel_order": "channels_first",
    "df_file": "./data/csv_shhs_stroke.csv",
    "data": {
      "train": [["shhs", "train"]],
      "eval": [["shhs", "eval"]],
      "test": [["shhs", "test"]]
    },
    "number_epochs": 1,
    "channels_to_load": ["ecg", "heartrate", "sao2"],
    "num_classes": 2
  }
}